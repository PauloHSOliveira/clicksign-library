{
  "name": "clicksign-library",
  "version": "0.1.2",
  "description": "This is a Node.js library to facilitate contract sending using the ClickSign REST API. The library is written in TypeScript and uses functional programming to provide an easy-to-use and scalable interface.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "private": false,
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "axios-mock-adapter": "^2.1.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.4.2",
    "rollup": "^4.28.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "format": "prettier --write \"**/*.{ts,json}\"",
    "start": "nodemon --exec ts-node src/index.ts"
  },
  "keywords": [
    "clicksign",
    "docusign",
    "docs",
    "react",
    "typescript",
    "node",
    "angular",
    "vue",
    "js",
    "framework js",
    "javascript",
    "typescript",
    "clicksign-library"
  ],
  "author": "PauloHSOliveira",
  "license": "MIT",
  "dependencies": {
    "ajv": "^8.17.1",
    "axios": "^1.7.9",
    "axios-rate-limit": "^1.4.0",
    "axios-retry": "^4.5.0",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./setupTests.ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PauloHSOliveira/clicksign-library.git"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
