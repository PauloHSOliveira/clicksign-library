{
  "name": "clicksign-library",
  "version": "0.1.1",
  "description": "This is a Node.js library to facilitate contract sending using the ClickSign REST API. The library is written in TypeScript and uses functional programming to provide an easy-to-use and scalable interface.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "private": true,
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.201",
    "@types/node": "^20.4.5",
    "axios-mock-adapter": "^1.21.5",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "rollup": "^4.4.1",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "format": "prettier --write \"**/*.{ts,json}\"",
    "start": "nodemon --exec ts-node src/index.ts"
  },
  "keywords": [
    "clicksign",
    "docusign",
    "docs",
    "react",
    "typescript",
    "node",
    "angular",
    "vue",
    "js",
    "framework js",
    "javascript",
    "typescript",
    "clicksign-library"
  ],
  "author": "PauloHSOliveira",
  "license": "MIT",
  "dependencies": {
    "ajv": "^8.12.0",
    "axios": "^1.5.0",
    "axios-rate-limit": "^1.3.0",
    "axios-retry": "^3.6.1",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./setupTests.ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PauloHSOliveira/clicksign-library.git"
  }
}
